    --project=qwiklabs-gcp-00-6399dbbc37f9 \
    --zone=us-central1-c \
    --machine-type=e2-micro \
    --network-interface=network=isaac-custom-vpc,subnet=isaac-subnet-central1 \
    --image-family=debian-11 \
    --image-project=debian-cloud \
    --boot-disk-size=20GB \
    --tags=allow-ssh \
    --description="VM in us-central1 connected to isaac-subnet-central1"

