# Permission to access the Terraform state bucket
gcloud storage buckets add-iam-policy-binding gs://learntfbucket \
  --member="serviceAccount:${SA_EMAIL}" \
  --role="roles/storage.objectAdmin"

# Permission for Cloud Build to write logs
gcloud projects add-iam-policy-binding $PROJECT_ID \
  --member="serviceAccount:${SA_EMAIL}" \
  --role="roles/logging.logWriter"


