export ORG_ID=32341657713

gcloud organizations add-iam-policy-binding $ORG_ID \
--member=serviceAccount:latchkey-multiorg-ephemeral-iam-automation@system.gserviceaccount.com \
--role=roles/resourcemanager.organizationAdmin --condition=None

gcloud organizations add-iam-policy-binding $ORG_ID \
--member=serviceAccount:latchkey-multiorg-ephemeral-custom-automation@system.gserviceaccount.com \
--role=roles/iam.organizationRoleAdmin --condition=None



